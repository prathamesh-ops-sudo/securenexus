{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "tsx script/build.ts",
    "start": "NODE_ENV=production node dist/index.cjs",
    "check": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check \"server/**/*.ts\" \"client/src/**/*.{ts,tsx}\" \"shared/**/*.ts\"",
    "format": "prettier --write \"server/**/*.ts\" \"client/src/**/*.{ts,tsx}\" \"shared/**/*.ts\"",
    "checks": "npm run typecheck && npm run lint && npm run test",
    "checks:all": "npm run typecheck && npm run lint && npm run test && npm run format:check",
    "db:push": "drizzle-kit push",
    "dev:setup": "bash scripts/dev-setup.sh",
    "dev:services": "docker compose up -d --wait",
    "dev:services:down": "docker compose down",
    "prepare": "node -e \"try{require('husky')}catch{process.exit(0)}\" && husky"
  },
